keywords = {differentially,expressed genes,metabolism,obstructive sleep apnea,protein-protein interaction network,transcription factor},
number = {22},
pages = {4293--4299},
pmid = {26636516},
title = {{Expression data analysis to identify key target genes in visceral fat tissue associated with obstructive sleep apnea.}},
volume = {19},
year = {2015}
