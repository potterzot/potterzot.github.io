mendeley-tags = {Bacillus thuringiensis,Bacillus thuringiensis: metabolism,Bacterial Proteins,Bacterial Proteins: metabolism,Colony Count,Endotoxins,Endotoxins: metabolism,Genetically Modified,Hemolysin Proteins,Hemolysin Proteins: metabolism,Hyphae,Hyphae: metabolism,Microbial,Mycorrhizae,Mycorrhizae: growth {\&} development,Plants,Soil Microbiology,Soybeans,Soybeans: growth {\&} development,Soybeans: microbiology,Zea mays,Zea mays: genetics,Zea mays: growth {\&} development,Zea mays: microbiology},
month = {apr},
