journal = {Animal : an international journal of animal bioscience},
keywords = {dna passage,genetically modified soybean meal,goat,kid},
mendeley-tags = {dna passage,genetically modified soybean meal,goat,kid},
month = {oct},
number = {10},
pages = {1662--71},
pmid = {22445119},
title = {{Fate of transgenic DNA and evaluation of metabolic effects in goats fed genetically modified soybean and in their offsprings.}},
