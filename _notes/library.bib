journal = {Proceedings of the National Academy of Sciences of the United States of America},
keywords = {Agricultural,Agricultural: genetics,Alleles,Animals,Bacillus thuringiensis,Bacillus thuringiensis: genetics,Bacterial Proteins,Bacterial Proteins: chemistry,Bacterial Proteins: pharmacology,Biological,Biological: methods,Crops,Endotoxins,Endotoxins: chemistry,Endotoxins: metabolism,Genetically Modified,Genetically Modified: genetics,Gossypium,Gossypium: genetics,Hemolysin Proteins,Hemolysin Proteins: chemistry,Inhibitory Concentration 50,Insecticide Resistance,Insecticides,Insecticides: pharmacology,Moths,Pest Control,Plants},
