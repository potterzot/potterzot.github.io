journal = {Journal of Chemical Health and Safety},
keywords = {and,conventional food-risk analysis,during the era of,expansion of,green revolution in the,introduction of high yielding,late sixties,pesticide residue in organic,ture,varieties},
month = {nov},
number = {6},
pages = {12--19},
title = {{Pesticide residue in organic and conventional food-risk analysis}},
