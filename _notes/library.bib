journal = {PloS one},
keywords = {Chloroplast,Chloroplasts,Chloroplasts: genetics,Cluster Analysis,DNA,Digestion,Female,Fetal Blood,Fetal Blood: chemistry,Food Habits,Genome,Humans,Inflammation,Inflammation: blood,Male,Plant,Plant: blood,Reproducibility of Results},
