keywords = {health,personal analytics,personal informatics,quantified self,self-experimentation,self-monitoring,self-tracking,user interfaces},
pages = {1143--1152},
title = {{Understanding Quantified-Selfers' Practices in Collecting and Exploring Personal Data}},
