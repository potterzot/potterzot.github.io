journal = {Journal of agricultural and food chemistry},
keywords = {Endotoxins,Endotoxins: analysis,Endotoxins: genetics,Endotoxins: metabolism,Gene Expression,Genetically Modified,Genetically Modified: chemistry,Genetically Modified: genetics,Genetically Modified: growth {\&} development,Genetically Modified: metabolism,Plants,Seasons,Zea mays,Zea mays: chemistry,Zea mays: genetics,Zea mays: growth {\&} development,Zea mays: metabolism},
month = {nov},
number = {21},
pages = {9990--6},
pmid = {19813729},
title = {{Expression of Cry3Bb1 in transgenic corn MON88017.}},
