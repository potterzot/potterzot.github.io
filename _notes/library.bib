volume = {43},
year = {2016}
}
@article{StormHeckeleiMittelhammer2016,
abstract = {We develop a Bayesian estimation framework for non-stationary Markov models for situations where both sample data on observed transitions between states (micro data) and population data, where only the proportion of individu- als in each state is observed (macro data), are available. Posterior distributions on transition probabilities are derived from a micro-based prior and a macro- based likelihood, thereby providing a new method that combines micro and macro information in a logically consistent manner and merges previously dis- parate approaches for inferring transition probabilities. Monte Carlo simula- tions for ordered and unordered states show how observed micro transitions improve the precision of posterior knowledge.},
