journal = {Environmental monitoring and assessment},
keywords = {Atrazine,Atrazine: analysis,District of Columbia,Environmental Monitoring,Fresh Water,Fresh Water: chemistry,Glycine,Glycine: analogs {\&} derivatives,Glycine: analysis,Maryland,Pesticides,Pesticides: analysis,United States,Water Pollutants, Chemical,Water Pollutants, Chemical: analysis,Wyoming},
month = {aug},
number = {1-4},
pages = {281--307},
pmid = {18677547},
title = {{The occurrence of glyphosate, atrazine, and other pesticides in vernal pools and adjacent streams in Washington, DC, Maryland, Iowa, and Wyoming, 2005-2006.}},
