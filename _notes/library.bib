@article{Cattani2014,
abstract = {Previous studies demonstrate that glyphosate exposure is associated with oxidative damage and neurotoxicity. Therefore, the mechanism of glyphosate-induced neurotoxic effects needs to be determined. The aim of this study was to investigate whether Roundup({\textregistered}) (a glyphosate-based herbicide) leads to neurotoxicity in hippocampus of immature rats following acute (30min) and chronic (pregnancy and lactation) pesticide exposure. Maternal exposure to pesticide was undertaken by treating dams orally with 1{\%} Roundup({\textregistered}) (0.38{\%} glyphosate) during pregnancy and lactation (till 15-day-old). Hippocampal slices from 15 day old rats were acutely exposed to Roundup({\textregistered}) (0.00005-0.1{\%}) during 30min and experiments were carried out to determine whether glyphosate affects (45)Ca(2+) influx and cell viability. Moreover, we investigated the pesticide effects on oxidative stress parameters, (14)C-$\alpha$-methyl-amino-isobutyric acid ((14)C-MeAIB) accumulation, as well as glutamate uptake, release and metabolism. Results showed that acute exposure to Roundup({\textregistered}) (30min) increases (45)Ca(2+) influx by activating NMDA receptors and voltage-dependent Ca(2+) channels, leading to oxidative stress and neural cell death. The mechanisms underlying Roundup({\textregistered})-induced neurotoxicity also involve the activation of CaMKII and ERK. Moreover, acute exposure to Roundup({\textregistered}) increased (3)H-glutamate released into the synaptic cleft, decreased GSH content and increased the lipoperoxidation, characterizing excitotoxicity and oxidative damage. We also observed that both acute and chronic exposure to Roundup({\textregistered}) decreased (3)H-glutamate uptake and metabolism, while induced (45)Ca(2+) uptake and (14)C-MeAIB accumulation in immature rat hippocampus. Taken together, these results demonstrated that Roundup({\textregistered}) might lead to excessive extracellular glutamate levels and consequently to glutamate excitotoxicity and oxidative stress in rat hippocampus.},
author = {Cattani, Daiane and {de Liz Oliveira Cavalli}, Vera L{\'{u}}cia and {Heinz Rieg}, Carla Elise and Domingues, Juliana Tonietto and Dal-Cim, Tharine and Tasca, Carla In{\^{e}}s and {Mena Barreto Silva}, F{\'{a}}tima Regina and Zamoner, Ariane},
