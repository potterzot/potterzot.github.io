journal = {Pest management science},
keywords = {Animals,Bacillus thuringiensis,Bacillus thuringiensis: genetics,Beetles,Beetles: drug effects,Beetles: growth {\&} development,Computer Simulation,Female,Insecticide Resistance,Larva,Male,Pest Control, Biological,Plants, Genetically Modified,Sex Ratio,Zea mays,Zea mays: genetics},
month = {jun},
