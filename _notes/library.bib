keywords = {Air Pollutants,Air Pollutants: analysis,Air Pollution,Air Pollution: statistics {\&} numerical data,Atmosphere,Atmosphere: chemistry,Environmental Monitoring,Glycine,Glycine: analogs {\&} derivatives,Glycine: analysis,Herbicides,Herbicides: analysis,Iowa,Mississippi,Organophosphonates,Organophosphonates: analysis,Weather},
month = {mar},
